{
  "name": "mapty-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build-only": "vite build",
    "preview": "vite preview",
    "deploy": "vite build && gh-pages -d dist",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix --cache",
    "format": "prettier --write src/"
  },
  "author": "Ralitsa Voronevska",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ralitsavoronevska/mapty-app.git"
  },
  "keywords": [
    "Vue.js 3 Composition API",
    "Tailwind CSS v4",
    "TypeScript",
    "Pinia",
    "Vite",
    "Vitest",
    "Responsiveness",
    "Transitions",
    "Animations",
    "Flex Layout",
    "Grid Layout"
  ],
  "dependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "leaflet": "^1.9.4",
    "pinia": "^3.0.4",
    "tailwindcss": "^4.1.17",
    "vue": "^3.5.25"
  },
  "devDependencies": {
    "@tsconfig/node24": "^24.0.3",
    "@types/jsdom": "^27.0.0",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/eslint-plugin": "^1.5.0",
    "@vitest/ui": "^4.0.15",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "~10.5.1",
    "gh-pages": "^6.3.0",
    "happy-dom": "^20.0.11",
    "jiti": "^2.6.1",
    "jsdom": "^27.2.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "typescript": "~5.9.0",
    "vite": "^7.2.4",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vitest": "^4.0.14",
    "vue-tsc": "^3.1.5"
  }
}
